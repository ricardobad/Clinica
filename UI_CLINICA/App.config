		<add name="win_auth" providerName="System.Data.SqlClient" connectionString="Data Source=DESKTOP-6RKSDTP\SQL2019_DEV;
																					Initial Catalog=CLINICA_SALUD_A_SU_ALCANCE; 
																					Integrated Security=True;"  />
		<!--Seguridad integrada de Windows-->

